<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!--================================ Diamond Ore ======================================-->
	
  <ThingDef ParentName="RockBase">
    <defName>MineableDiamond</defName>
    <label>алмазная руда</label>
    <description>Порода, содержащая в себе алмазы.</description>
    <graphicData>
      <texPath>Things/Building/Linked/RockFlecked_Atlas</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <color>(114,114,114)</color>
      <colorTwo>(184,225,225)</colorTwo>
    </graphicData>
    <statBases>
      <MaxHitPoints>3000</MaxHitPoints>
    </statBases>
    <building>
      <isResourceRock>true</isResourceRock>
      <mineableThing>Diamond</mineableThing>
      <mineableYield>1</mineableYield>
      <mineableScatterCommonality>0.0050</mineableScatterCommonality>
      <mineableScatterLumpSizeRange>
        <min>1</min>
        <max>3</max>
      </mineableScatterLumpSizeRange>
    </building>
  </ThingDef>
  
